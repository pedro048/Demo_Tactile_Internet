        <script src="/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
        <script src="https://cdn.socket.io/4.5.4/socket.io.min.js"></script>
        <!--<script src="/dist/easyzoom.js"></script>-->
        <script>
            // Instantiate EasyZoom instances
            //var $easyzoom = $('.easyzoom').easyZoom();

            const socket = io();

            var oldSpeedValue = 0;
            var mouseSpeed=0;
            
            var idTactile = '0';
            
            //-----------------------------------------------------------
            
            document.querySelector("[name='btn_on_id']").value = '1';
            document.querySelector("[name='btn_off_id']").value = '0';

            function clickButtonOff(){
                var button = document.querySelector("[name='insert_off']");
                button.click();
            }
            
            //-----------------------------------------------------------
            
            function hoverFunction_absence_of_sensation(){
                document.querySelector("[name='id_zero']").value = '0';
                var button = document.querySelector("[name='insert_0']");
                button.click();  
            }   

            function hoverFunction_CR(){  
                document.querySelector("[name='id_one']").value = '1';
                var button = document.querySelector("[name='insert_1']");
                button.click();
            }    

            function hoverFunction_FR(){  
                document.querySelector("[name='id_two']").value = '2';
                var button = document.querySelector("[name='insert_2']");
                button.click();
            }

            function hoverFunction_SM(){ 
                document.querySelector("[name='id_three']").value = '3';
                var button = document.querySelector("[name='insert_3']");
                button.click();  
            }

            function hoverFunction_SO(){   
                document.querySelector("[name='id_four']").value = '4';
                var button = document.querySelector("[name='insert_4']");
                button.click();
            } 
        
            //-----------------------------------------------------------
            
            function hoverFunctionPassive_absence_of_sensation(){
                idTactile = '0';
            }

            function hoverFunctionPassive_CR(){  
                idTactile = '1';
            }    

            function hoverFunctionPassive_FR(){  
                idTactile = '2';
            }

            function hoverFunctionPassive_SM(){ 
                idTactile = '3';     
            }

            function hoverFunctionPassive_SO(){   
                idTactile = '4';
            }
        
            //-----------------------------------------------------------    
            
            var image_left = document.getElementById("left_image");
            var image_center = document.getElementById("center_image");
            var image_right = document.getElementById("right_image"); 

            function imageWithoutZoom(){
                if('<%- val_ON_OFF %>' == "0"){    
                    image_left.className = "img-row-all_1";
                    image_center.className = "hide";
                    image_right.className = "hide";
                }
                
                if('<%- val_ON_OFF %>' == "1"){
                    image_left.className = "img-row-all";
                    image_center.className = "hide";
                    image_right.className = "hide";
                }    
            }    
            
            function imageWithTopZoom(){
                if('<%- val_ON_OFF %>' == "0"){    
                    image_left.className = "hide";
                    image_center.className = "img-row-all_1";
                    image_right.className = "hide";
                }
                
                if('<%- val_ON_OFF %>' == "1"){
                    image_left.className = "hide";
                    image_center.className = "img-row-all";
                    image_right.className = "hide";
                }
            }    
            
            function imageWithBottomZoom(){ 
                if('<%- val_ON_OFF %>' == "0"){   
                    image_left.className = "hide";
                    image_center.className = "hide";
                    image_right.className = "img-row-all_1";
                }

                if('<%- val_ON_OFF %>' == "1"){
                    image_left.className = "hide";
                    image_center.className = "hide";
                    image_right.className = "img-row-all";
                }
            }     
            
            //-----------------------------------------------------------
            
            var prevEvent, currentEvent;
            var x_passive=0, y_passive=0;
                
            document.documentElement.onmousemove=function(e){
                x_passive = e.pageX;
                //console.log("x: " + x_passive);
                y_passive = e.pageY;
                //console.log("y: " + y_passive);
                   
                var passive_touch = document.getElementById("passive_touch");
                passive_touch.style.left = (x_passive + 10) + "px";
                passive_touch.style.top = (y_passive + 10) + "px";
                
                currentEvent=e;
            }

            function trackMouseSpeedFunc(){       
                if(prevEvent && currentEvent){ 
                    var movementX=Math.abs(currentEvent.screenX-prevEvent.screenX);
                    var movementY=Math.abs(currentEvent.screenY-prevEvent.screenY);
                    var movement=Math.sqrt(movementX*movementX+movementY*movementY); 
                    //speed=movement/100ms= movement/0.1s= 10*movement/s
                    var speed=10*movement;//current speed
                }

                if(oldSpeedValue == 0){
                    mouseSpeed = speed;
                }else{
                    mouseSpeed = (speed + oldSpeedValue)/2;
                } 

                oldSpeedValue = speed;
                //console.log("mouseSpeed: ", mouseSpeed);

                socket.emit('mouse_speed_track', mouseSpeed, idTactile);
                       
                prevEvent=currentEvent;
            }

            setInterval(trackMouseSpeedFunc, 500);
             
            //-----------------------------------------------------------    
        
            function disableScroll() {
                // Get the current page scroll position
                scrollTop =
                window.pageYOffset || document.documentElement.scrollTop;
                scrollLeft =
                window.pageXOffset || document.documentElement.scrollLeft,
          
                // if any scroll is attempted,
                // set this to the previous value
                window.onscroll = function() {
                    window.scrollTo(scrollLeft, scrollTop);
                };
            }
          
            function enableScroll() {
                window.onscroll = function() {};
            }
            
            //#######################################################################
            
            if('<%- val_ON_OFF %>' == "1"){
                disableScroll();
                
                var x_active=0, y_active=0;
                var cursorLeft=0, cursorRight=0, cursorTop=0, cursorBottom=0;
                // Image of cursor
                var cursor = document.getElementById("cursorActive");
                cursor.className = "";
                
                // Gets the x,y position of the image cursor
                x_active = '<%- val_X %>';
		        y_active = '<%- val_Y %>';

                // Sets the image cursor to new position
                cursor.style.left = x_active + "px";
		        cursor.style.top = y_active + "px";
                
                cursorLeft = Number(x_active);
                cursorRight = cursorLeft + 10;
                cursorTop = Number(y_active);
                cursorBottom = cursorTop + 12; 
                
                var img_1_id = document.getElementById("img_1_id");
                var coordinatesImg_1_id = img_1_id.getBoundingClientRect();
                
                var img_2_id = document.getElementById("img_2_id");
                var coordinatesImg_2_id = img_2_id.getBoundingClientRect();

                var img_3_id = document.getElementById("img_3_id");
                var coordinatesImg_3_id = img_3_id.getBoundingClientRect();

                var img_4_id = document.getElementById("img_4_id");
                var coordinatesImg_4_id = img_4_id.getBoundingClientRect();

                var img_5_id = document.getElementById("img_5_id");
                var coordinatesImg_5_id = img_5_id.getBoundingClientRect();

                var img_6_id = document.getElementById("img_6_id");
                var coordinatesImg_6_id = img_6_id.getBoundingClientRect();

                var img_7_id = document.getElementById("img_7_id");
                var coordinatesImg_7_id = img_7_id.getBoundingClientRect();

                var img_8_id = document.getElementById("img_8_id");
                var coordinatesImg_8_id = img_8_id.getBoundingClientRect();

                var img_9_id = document.getElementById("img_9_id");
                var coordinatesImg_9_id = img_9_id.getBoundingClientRect();

                var img_10_id = document.getElementById("img_10_id");
                var coordinatesImg_10_id = img_10_id.getBoundingClientRect();

                
                //----------------------------------------------- Inside the image
                if(cursorLeft > Number(coordinatesImg_1_id.left) && cursorRight < Number(coordinatesImg_1_id.right) && cursorBottom < Number(coordinatesImg_1_id.bottom) && cursorTop > Number(coordinatesImg_1_id.top)){
                    imageWithoutZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorLeft > Number(coordinatesImg_2_id.left) && cursorRight < Number(coordinatesImg_2_id.right) && cursorBottom < Number(coordinatesImg_2_id.bottom) && cursorTop > Number(coordinatesImg_2_id.top)){
                    imageWithTopZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorLeft > Number(coordinatesImg_3_id.left) && cursorRight < Number(coordinatesImg_3_id.right) && cursorBottom < Number(coordinatesImg_3_id.bottom) && cursorTop > Number(coordinatesImg_3_id.top)){
                    imageWithoutZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorLeft > Number(coordinatesImg_4_id.left) && cursorRight < Number(coordinatesImg_4_id.right) && cursorBottom < Number(coordinatesImg_4_id.bottom) && cursorTop > Number(coordinatesImg_4_id.top)){
                    imageWithTopZoom();
                    hoverFunction_SM();
                }
                if(cursorLeft > Number(coordinatesImg_5_id.left) && cursorRight < Number(coordinatesImg_5_id.right) && cursorBottom < Number(coordinatesImg_5_id.bottom) && cursorTop > Number(coordinatesImg_5_id.top)){
                    imageWithTopZoom();
                    hoverFunction_SM();
                }
                if(cursorLeft > Number(coordinatesImg_6_id.left) && cursorRight < Number(coordinatesImg_6_id.right) && cursorBottom < Number(coordinatesImg_6_id.bottom) && cursorTop > Number(coordinatesImg_6_id.top)){
                    imageWithTopZoom();
                    hoverFunction_SM();
                }
                if(cursorLeft > Number(coordinatesImg_7_id.left) && cursorRight < Number(coordinatesImg_7_id.right) && cursorBottom < Number(coordinatesImg_7_id.bottom) && cursorTop > Number(coordinatesImg_7_id.top)){
                    imageWithBottomZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorLeft > Number(coordinatesImg_8_id.left) && cursorRight < Number(coordinatesImg_8_id.right) && cursorBottom < Number(coordinatesImg_8_id.bottom) && cursorTop > Number(coordinatesImg_8_id.top)){
                    imageWithBottomZoom();
                    hoverFunction_SM();
                }
                if(cursorLeft > Number(coordinatesImg_9_id.left) && cursorRight < Number(coordinatesImg_9_id.right) && cursorBottom < Number(coordinatesImg_9_id.bottom) && cursorTop > Number(coordinatesImg_9_id.top)){
                    imageWithBottomZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorLeft > Number(coordinatesImg_10_id.left) && cursorRight < Number(coordinatesImg_10_id.right) && cursorBottom < Number(coordinatesImg_10_id.bottom) && cursorTop > Number(coordinatesImg_10_id.top)){
                    imageWithBottomZoom();
                    hoverFunction_FR();
                }
                //----------------------------------------------- Outside the image
                if(cursorTop < Number(coordinatesImg_2_id.top)){
                    imageWithoutZoom();
                    hoverFunction_absence_of_sensation();
                }
                if(cursorBottom > Number(coordinatesImg_10_id.bottom)){
                    imageWithoutZoom();
                    hoverFunction_absence_of_sensation();
                }             
            }
        
            if('<%- val_ON_OFF %>' == "1"){
                function Refresh(){
                    location.reload();
                }
                setInterval(Refresh, 200);
            }
            
        </script>
